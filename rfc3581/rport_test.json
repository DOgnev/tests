"Tests": [
    {
      "Name": "call_with_rport",
      "Description": "Подставляем в Via левый порт и пустой rport. Ответы должны будут приходить на порт из rport (значение порта подставляет сервер).",
      "TestProcedure": [
      {
          "StartUA": [
            {
              "Type" : "User",
              "UserId" : 1,
              "WriteStat" : true,
              "Name": "UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            },
            {
              "Type" : "User",
              "UserId" : 0,
              "WriteStat" : true,
              "Name": "UAC_SEND_RPORT",
              "Commands": [
                {
                  "SourceFile": "rfc3581/uac_rport.xml",
                  "Options": "-m 1 -set CDPN %%1.Number%% -set CDPNDOM %%1.SipDomain%% -set FAKE_PORT 1000",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "call_without_rport",
      "Description": "Подставляем в Via левый порт. Ответы не должны приходить",
      "TestProcedure": [
      {
          "StartUA": [
            {
              "Type" : "User",
              "UserId" : 0,
              "WriteStat" : true,
              "Name": "UAC_SEND_RPORT",
              "Commands": [
                {
                  "SourceFile": "rfc3581/uac_no_answer.xml",
                  "Options": "-m 1 -set CDPN %%1.Number%% -set CDPNDOM %%1.SipDomain%% -set FAKE_PORT 1000",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}