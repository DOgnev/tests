      "command3": "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% clir mode = restrict_all",
      "command4": "/domain/%%DEV_DOM%%/ss/activate %%1.Number%% clip",
      "command5": "/domain/%%DEV_DOM%%/ss/activate %%1.Number%% cnip",
      "command6": "/domain/%%DEV_DOM%%/ss/activate %%1.Number%% cliro"
