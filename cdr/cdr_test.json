  "Tests": [
    {
      "Name": "simple_cdr_test",
      "Description": "\u0417\u0430\u0432\u043e\u0434\u0438\u043c cdr \u0433\u0440\u0443\u043f\u043f\u0443. \u041f\u0440\u0438\u0432\u044f\u0437\u044b\u0432\u0430\u0435\u043c \u0435\u0451 \u043a \u0430\u0431\u043e\u043d\u0435\u0442\u0443. \u0414\u0435\u043b\u0430\u0435\u043c \u0432\u044b\u0437\u043e\u0432 \u0441 \u044d\u0442\u043e\u0433\u043e \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u0430. \u0417\u0430\u0431\u0438\u0440\u0430\u0435\u043c cdr \u0444\u0430\u0439\u043b \u0438 \u043f\u0440\u043e\u0432\u0435\u0440\u044f\u0435\u043c \u043f\u043e\u043b\u044f KOD_A,KOD_B",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "WriteStat": true,
              "Name": "UAS_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -rtp_echo -d 1000",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
              "Name": "UAC_TEST",
              "Commands": [
                {
                  "SourceFile": "example/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            }
          ]
        },
        {
          "CompareCDR": {
            "CDRGroup": "%%CDR_GROUP%%",
            "CDRParams": {
              "KOD_A": "%%1.Number%%",
              "KOD_B": "%%0.Number%%"
            }
          }
        }
      ]
    }
