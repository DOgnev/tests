{
  "TestName": "diversion_header",
  "Users" : [
  ],
  "Trunks" : [
    {
      "TrunkId" : 1,
      "TrunkName" : "diver_tr1",
      "Port" : 5094,
      "SipDomain" : "pv.ssw3",
      "SipGroup" : "diver_gr"
    },
    {
      "TrunkId" : 2,
      "TrunkName" : "diver_tr2",
      "Port" : 5095,
      "SipDomain" : "pv.ssw3",
      "SipGroup" : "diver_gr"
    }
   ],

  "UserVar": [
    {
      "%%to_trunk_ctx%%" : "",
      "%%trunk_ctx_name%%" : "diver_trunk_ctx"
    }
  ],
  "PreConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/routing/.xbin/.import-context --xml-base64 %%to_trunk_ctx%%",
      "command2" : "/domain/%%DEV_DOM%%/trunk/sip/declare %%trunk_ctx_name%% %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.1.Port%% sip-proxy %%EXTER_PORT%%",
      "command3" : "/domain/%%DEV_DOM%%/trunk/sip/declare %%SSW_DEF_ROUTE%% %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.2.Port%% sip-proxy %%EXTER_PORT%%",
      "command4" : "/domain/%%DEV_DOM%%/routing/show %%trunk_ctx_name%%"
    }
  ],
  "PostConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% --force",
      "command2" : "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% --force"
    }
  ],
"Tests": [
    {
      "Name": "diversion_header_test",
      "Description": "Прверка того, что ssw делает вызов на номер из RURI, а не из DIVERSION",
      "TestProcedure": [
      {
          "StartUA": [
            {
              "Type" : "Trunk",
              "TrunkId" : 2,
              "WriteStat" : true,
              "Name": "UAS_TRUNK_1",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1 -set CHK_CGPN 100",
                  "SippType": "uas",
                  "Timeout": "60s"
                }
              ]
            },
            {
              "Type" : "Trunk",
              "TrunkId" : 1,
              "WriteStat" : true,
              "Name": "UAC_TRUNK",
              "Commands": [
                {
                  "SourceFile": "diversion_header/uac_send_diversion_hdr.xml",
                  "Options": "-m 1 -set CDPN 100 -set CDPNDOM %%Tr.1.SipDomain%% -set CGPNDOM %%Tr.1.SipDomain%% -set CGPN 101 -set DIVER_NUM 655 -d 5000",
                  "SippType": "uac",
                  "Timeout": "60s"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}