  "Tests": [
    {
      "Name": "simple_last_number_redial_test",
      "Description": "\u0410\u043a\u0442\u0438\u0432\u0438\u0440\u0443\u0435\u043c \u0443\u0441\u043b\u0443\u0433\u0443 speed_dial. \u0414\u0435\u043b\u0430\u0435\u043c \u0432\u044b\u0437\u043e\u0432\u044b \u043d\u0430 \u043a\u043e\u0440\u043e\u0442\u043a\u0438\u0435 \u043d\u043e\u043c\u0435\u0440\u0430. \u0414\u0435\u0430\u043a\u0442\u0438\u0432\u0438\u0440\u0443\u0435\u043c \u0443\u0441\u043b\u0443\u0433\u0443 \u0438 \u043f\u0440\u043e\u0432\u0435\u0440\u044f\u0435\u043c, \u0447\u0442\u043e \u043e\u043d\u0430 \u0434\u0435\u0430\u043a\u0442\u0438\u0432\u0438\u0440\u043e\u0432\u0430\u043b\u0430\u0441\u044c",
      "TestProcedure": [
        {
          "ServiceFeature": [
            {
              "code": "*51*1*%%1.Number%%#",
              "userId": 0
            }
          ]
        },
        {
          "ServiceFeature": [
            {
              "code": "*51*2*%%2.Number%%#",
              "userId": 0
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 1,
              "Name": "UAS1_SIMPLE_REDIAL_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 2000",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 2,
              "Name": "UAS2_SIMPLE_REDIAL_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 2000",
                  "SippType": "uas",
