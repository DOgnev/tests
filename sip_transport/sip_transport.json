        {
          "CoconCommand": [
            {
              "CoconCommand": "/domain/%%DEV_DOM%%/sip/properties/set sip_transport tcp_only"
            }
          ]
        },
        {
          "Sleep": "2"
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": "0",
              "Name": "TCP_UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -aa",
                  "SippType": "uas",
                  "Timeout": "30s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "1",
              "WriteStat": "True",
              "Name": "TCP_UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%% -aa",
                  "SippType": "uac",
                  "NeedAuth": "True",
                  "Timeout": "30s"
                },
                {
                  "SourceFile": "sip_transport/uac_recv_480.xml",
                  "Options": "-m 1 -set CDPN %%2.Number%% -set CDPNDOM %%2.SipDomain%% -aa",
