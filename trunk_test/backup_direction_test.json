{
  "TestName": "sip_transport_test",
  "Trunks" : [
    {
      "TrunkId" : 1,
      "TrunkName" : "sip_tr_trunk_1",
      "Port" : 5094,
      "SipDomain" : "pv.ssw3",
      "SipGroup" : "sip_transport"
    },
    {
      "TrunkId" : 2,
      "TrunkName" : "sip_tr_trunk_2",
      "Port" : 5095,
      "SipDomain" : "pv.ssw3",
      "SipGroup" : "sip_transport",
      "RemotePort" : 5062
    }
   ],

"UserVar": [
    {
      "%%to_trunk2_ctx%%" : "",
      "%%to_trunk1_ctx%%" : "",
      "%%trunk1_ctx_name%%" : "sip_transport_trunk1_test",
      "%%trunk2_ctx_name%%" : "sip_transport_trunk2_test"
    }
],

  "PreConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/routing/.xbin/.import-context --xml-base64 %%to_trunk2_ctx%%",
      "command2" : "/domain/%%DEV_DOM%%/routing/.xbin/.import-context --xml-base64 %%to_trunk1_ctx%%",
      "command3" : "/domain/%%DEV_DOM%%/trunk/sip/declare %%trunk1_ctx_name%% %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.1.Port%% sip-proxy %%EXTER_PORT%%",
      "command4" : "/domain/%%DEV_DOM%%/trunk/sip/declare %%trunk2_ctx_name%% %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.2.Port%% sip-proxy %%Tr.2.RemotePort%%"
    }
  ],
  "PostConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% --force",
      "command2" : "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% --force",
      "command3" : "/domain/%%DEV_DOM%%/routing/delete %%trunk1_ctx_name%%",
      "command4" : "/domain/%%DEV_DOM%%/routing/delete %%trunk2_ctx_name%%"
    }
  ],
  "Tests": [
    {
      "Name": "test_tcp_only_mode",
      "Description": "Проверка режима tcp_only.",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/trunk/sip/set %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% sip_transport tcp_only"
            }
          ]
        },
        {
          "Sleep" : 2
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 2,
              "Name": "TCP_UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -aa -t tn -max_socket 25 -d 2000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId" : 1,
              "WriteStat": true,
              "Name": "TCP_UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN 2000 -set CDPNDOM %%Tr.1.SipDomain%% -set CGPN 1000 -set CGPNDOM %%Tr.2.SipDomain%% -aa -t tn -max_socket 25",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 1,
              "Name": "UDP_UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -aa -d 2000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId" : 2,
              "WriteStat": true,
              "Name": "TCP_UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN 2000 -set CDPNDOM %%Tr.1.SipDomain%% -set CGPN 1000 -set CGPNDOM %%Tr.2.SipDomain%% -aa -t tn -max_socket 25",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 1,
              "WriteStat": true,
              "Name": "TCP_UAC_RECV_502",
              "Commands": [
                {
                  "SourceFile": "sip_transport/uac_recv_502.xml",
                  "Options": "-m 1 -set CDPN 2000 -set CDPNDOM %%Tr.1.SipDomain%% -set CGPN 1000 -set CGPNDOM %%Tr.2.SipDomain%% -aa -t tn -max_socket 25",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 2,
              "WriteStat": true,
              "Name": "UDP_UAC_NO_ANSW_TO_INVITE",
              "Commands": [
                {
                  "NoTimeOutError" : true,
                  "SourceFile": "sip_transport/uac_no_answ_to_invite.xml",
                  "Options": "-m 1 -set CDPN 2000 -set CDPNDOM %%Tr.1.SipDomain%% -set CGPN 1000 -set CGPNDOM %%Tr.2.SipDomain%% -aa",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}