                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "30s"
                },
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1  -d 1000 -set CHK_DNAME Callback -set CHK_CGPN %%2.Number%%",
