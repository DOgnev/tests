                }
              ]
            },
            {
              "Type": "User",
              "UserId": "0",
              "Name": "UAC_AUTOANSWER_VP_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN *80*%%1.Number%%# -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": "True",
                  "Timeout": "20s"
                }
              ]
            }
          ]
        }
