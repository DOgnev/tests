              "command2": "/domain/%%DEV_DOM%%/routing/.xbin/.import-context --xml-base64 ",
              "command3": "/domain/%%DEV_DOM%%/iface/user-set sip1 %%4.SipGroup%% %%4.Number%%@%%4.SipDomain%% routing.context cgg_test_ctx"
