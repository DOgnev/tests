  "UserVar": [
    {
      "%%CggGroup%%": "[%%1.Number%%,%%2.Number%%,%%3.Number%%]",
      "%%CggUser%%": "%%0.Number%%",
      "%%CggSipDomain%%": "%%0.SipDomain%%",
      "%%CggSipGroup%%" : "%%0.SipGroup%%",
      "%%SIP_DNAME%%" : "CggSipDNAME",
      "%%ALIAS_DNAME%%" : "AliasDNAME"
    }
  ],
  "PreCoconConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/ss/enable %%CggUser%% cgg",
      "command2": "/domain/%%DEV_DOM%%/ss/activate %%CggUser%% cgg numbers = %%CggGroup%%"
    }
  ],
  "PostCoconConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/ss/deactivate %%CggUser%% cgg",
      "command2": "/domain/%%DEV_DOM%%/ss/disable %%CggUser%% cgg"
    }
  ],
  "Tests": [
    {
      "Name": "cgg_dname_from_sip",
      "Description": "dName получен через SIP, на абоненте не выставлен, на маршрутизации не выставлен. На стороне Б ожидаем dName с SIP",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": "1",
              "WriteStat" : "True",
              "Name": "UAS1_CGG_WOUT100REL",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1 -set CHK_DNAME %%SIP_DNAME%%",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "2",
              "WriteStat" : "True",
              "Name": "UAS2_CGG_WOUT100REL",
              "Commands": [
                {
                  "SourceFile": "cgg/uas_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "40s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "3",
              "WriteStat" : "True",
              "Name": "UAS3_CGG_WOUT100REL",
              "Commands": [
                {
                  "SourceFile": "cgg/uas_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "40s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "4",
              "WriteStat" : "True",
              "Name": "UAC_WOUT100REL",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_bye.xml",
                  "Options": "-m 1 -set CDPN %%CggUser%% -set CDPNDOM %%CggSipDomain%% -set DISPLAY_NAME %%SIP_DNAME%%",
                  "SippType": "uac",
                  "NeedAuth": "True",
                  "Timeout": "40s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "cgg_dname_from_alias",
      "Description": "dName получен через SIP, на абоненте выставлен, на маршрутизации не выставлен. На стороне Б ожидаем dName с alias",
