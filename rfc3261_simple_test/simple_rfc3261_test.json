  "UserVar": [
    {
    }
  ],
  "PreCoconConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/ss/enable {%%0.Number%%-%%1.Number%%} clip",
      "command2" : "/domain/%%DEV_DOM%%/ss/activate {%%0.Number%%-%%1.Number%%} clip"
    }
  ],
  "PostCoconConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/ss/deactivate {%%0.Number%%-%%1.Number%%} clip",
      "command2" : "/domain/%%DEV_DOM%%/ss/enable {%%0.Number%%-%%1.Number%%} clip"
