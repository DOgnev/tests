            {
              "Type": "User",
              "UserId" : 0,
              "Name": "UAS_ATT_TRANSFER",
              "Commands": [
                {
                  "SourceFile": "ss_transfer/attended_transfer.xml",
                  "Options": "-m 1 -set REFERNUM %%1.Number%% -set REFERDOM %%1.SipDomain%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "30s"
                }
              ]
