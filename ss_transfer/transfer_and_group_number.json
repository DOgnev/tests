  "Tests": [
    {
      "Name": "transfer_incoming_call_from_cgg",
      "Description": "Проверка корректности номера при трансфере вызова, пришедшего из cgg группы.",
      "TestProcedure": [
        {
          "SendSSHCommand" : [
            {
              "command1" : "/domain/%%DEV_DOM%%/ss/activate %%1.Number%% cgg numbers = [%%2.Number%%,%%3.Number%%]"
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 4,
              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 7000 -set CHK_DNAME \"%%CGG_GROUP_USER_DNAME%%\" -set CHK_CGPN %%2.Number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "30s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 2,
              "NeedAuth" : true,
              "Name": "UAS_AT_TRANS",
              "Commands": [
                {
                  "SourceFile": "ss_transfer/unattended_transfer.xml",
                  "Options": "-m 1 -set REFERNUM %%4.Number%% -set REFERDOM %%4.SipDomain%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "30s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 3,
              "Name": "UAS_RECV_CANCEL",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "30s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 0,
              "Name": "SIMPLE_UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%1.Number%% -set CDPNDOM %%1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "30s"
                }
              ]
            }
          ]
        },
        {
          "SendSSHCommand" : [
            {
              "command1" : "/domain/%%DEV_DOM%%/ss/deactivate %%1.Number%% cgg"
            }
          ]
        }
      ]
    },
