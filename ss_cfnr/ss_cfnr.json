    {
      "Name": "simple_cfnr_test",
      "Description": "Звоним на абонента с cfnr, не отвечаем и проверяем переадресацию",
      "TestProcedure": [
        {
          "ServiceFeature": [
            {
              "code": "*27*%%0.Number%%#",
              "userId" : 2
            }
          ]
        },
        {
