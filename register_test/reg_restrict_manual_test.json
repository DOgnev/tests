  "Tests": [
    {
      "Name": "Проверка корректности детектирования ВАТС (одинаковые логины)",
      "Description" : "В разных доменах есть одинаковые юзеры с разными паролями. В данном тесте делается попытка вызова юзера с одного домена в другом домене.",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 2,
              "Name": "UAC1_TEST_CALL",
              "Commands": [
                 {
                  "SourceFile": "register_test/uac_recv_403.xml",
                  "Options": "-m 1 -set CDPN %%2.Number%% -set CDPNDOM pbx2.test.loc",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 3,
              "Name": "UAC2_TEST_CALL",
              "Commands": [
                 {
                  "SourceFile": "register_test/uac_recv_403.xml",
                  "Options": "-m 1 -set CDPN %%3.Number%% -set CDPNDOM pbx1.test2.loc",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "Проверка корректности детектирования ВАТС (матчинг ВАТС)",
      "Description" : "Попытка позвонить на юзера с состовным CDPNDOMAIN",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 2,
              "Name": "UAC1_TEST_CALL",
              "Commands": [
                 {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%1.Number%% -set CDPNDOM %%1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 1,
              "Name": "UAS2_TEST_CALL",
              "Commands": [
                 {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 1,
              "Name": "UAC1_TEST_CALL",
              "Commands": [
                 {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%2.Number%% -set CDPNDOM %%2.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 2,
              "Name": "UAS2_TEST_CALL",
              "Commands": [
                 {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "register_foreign_domain_in_ruri",
      "Description" : "Отправляем REGISTER с произвольным доменом в RURI, который не должен попадать в список allowed_ruri",
      "TestProcedure": [
        {
          "ManualReg": {
          "4": {
              "script": "register_test/send_reg_with_foreign_ruri.xml",
              "additional_options" : "-set FOREIGN_DOM %%DEV_DOM%%.faka"
            }
          }
        },
        {
          "ManualReg": {
          "4": {
              "script": "register_test/send_reg_with_foreign_ruri.xml",
              "additional_options" : "-set FOREIGN_DOM faka.%%DEV_DOM%%"
            }
          }
        }
      ]
    }
