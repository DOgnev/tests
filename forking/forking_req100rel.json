      "Name": "forking_all-contact_req100rel_test",
      "Description": "Звоним на юзеров с поддержкой 100rel, даём a_sdp в 180 сообщении, после чего с одного из ua отвечаем, остальным CANCEL",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% fork-mode all-contacts"
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "WriteStat": true,
              "Name": "UAS0_FORK_REQ100REL",
              "Commands": [
                {
                  "SourceFile": "forking/req100rel/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                },
                {
                  "SourceFile": "forking/req100rel/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                },
                {
                  "SourceFile": "simple_ua/req100rel/uas_send_bye.xml",
                  "Options": "-m 1 -d 5000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
