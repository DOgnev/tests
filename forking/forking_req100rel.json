  "Tests": [
    {
      "Name": "forking_all-contact_req100rel_test",
      "Description": "Звоним на юзеров с поддержкой 100rel, даём a_sdp в 180 сообщении, после чего с одного из ua отвечаем, остальным CANCEL",
      "TestProcedure": [
        {
          "CoconCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% fork-mode all-contacts"
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": "0",
              "WriteStat": "True",
              "Name": "UAS0_REQ100REL_RECV_CANEL",
              "Commands": [
                {
                  "SourceFile": "forking/req100rel/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "1",
              "WriteStat": "True",
              "Name": "UAS1_REQ100REL_RECV_CANEL",
              "Commands": [
                {
                  "SourceFile": "forking/req100rel/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "2",
              "WriteStat": "True",
              "Name": "UAS2_REQ100REL_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "forking/req100rel/uas_send_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "3",
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": "True",
                  "Timeout": "20s"
                }
              ]
            }
          ]
        },
        {
          "CheckDifference": [
            {
              "Difference": "0",
              "Mode" : "between_ua",
              "UA": "0,1,2",
              "Msg" : [
                {
                  "MsgType": "request",
                  "Method" : "INVITE",
                  "Code" : "None"
                }
              ]
            },
            {
              "Difference": "0",
              "Mode" : "between_ua",
              "UA": "0,1",
              "Msg" : [
                {
                  "MsgType": "request",
                  "Method" : "CANCEL",
                  "Code" : "None"
                }
              ]
            }
          ]
        }
      ]
    },
