              "Type": "User",
              "UserId": 3,
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/req100rel/uac_recv_bye.xml",
                  "Options": "-m 3 -r 1 -l 1 -d 3000 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            }
          ]
        },
        {
