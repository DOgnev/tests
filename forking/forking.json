    {
      "Name": "forking all-contact test",
      "Description": "Звоним за номер с форкингом и отвечаем с одного из контактов. Вызов проключается, всем остальным высылается CANCEL",
      "TestProcedure": [
        {
          "CoconCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% fork-mode all-contacts"
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": "0",
              "WriteStat": "True",
              "Name": "UAS1_RECV_CANEL",
              "Commands": [
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "1",
              "WriteStat": "True",
              "Name": "UAS2_RECV_CANEL",
              "Commands": [
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "2",
              "WriteStat": "True",
              "Name": "UAS3_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "3",
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": "True",
                  "Timeout": "20s"
                }
              ]
            }
          ]
        },
        {
          "CheckDifference": [
            {
              "Method": "INVITE",
              "Difference": "0",
              "UA": "0,1,2"
            },
            {
              "Method": "CANCEL",
              "Difference": "0",
              "UA": "0,1"
            }
          ]
        }
      ]
    },
