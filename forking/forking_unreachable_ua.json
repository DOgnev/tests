  "Tests": [
    {
      "Name": "forking_two_ua_unreachable",
      "Description": "Звоним на номер с форкингом при этом делаем несколько контактов недоступными. После 32 секунд отвечаем на вызов.",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% fork-mode all-contacts"
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 2,
              "WriteStat": true,
              "Name": "UAS_DELAYED_ANSWER",
              "Commands": [
                {
                  "SourceFile": "forking/uas_delayed_answer.xml",
                  "Options": "-m 1 -d 33000",
                  "SippType": "uas",
                  "Timeout": "60s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 3,
              "Name": "UAC_RECV_BYE",
