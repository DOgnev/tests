    {
      "command1" : "/domain/%%DEV_DOM%%/routing/.xbin/.import-context --xml-base64 %%to_trunk_ctx%%",
      "command2" : "/domain/%%DEV_DOM%%/trunk/sip/declare %%ctx_name%% %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.1.Port%% sip-proxy %%EXTER_PORT%%",
      "command3" : "/domain/%%DEV_DOM%%/iface/user-set sip1 %%1.SipGroup%% %%1.Number%%@%%1.SipDomain%% routing.context %%ctx_name%%"
    }
  ],
  "PostConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% --force",
      "command2" : "/domain/%%DEV_DOM%%/iface /user-set sip1 %%1.SipGroup%% %%1.Number%%@%%1.SipDomain%% routing.context default.routing",
      "command3": "/domain/%%DEV_DOM%%/routing/delete %%ctx_name%%"
    }
  ],
