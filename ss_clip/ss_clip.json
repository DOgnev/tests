{
  "TestName": "ss_clip",
  "AutoTest": "True",
  "Users": [
    {
      "UserId": "0",
      "Number": "156",
      "Login": "156",
      "Password": "123",
      "SipDomain": "pv.ssw3",
      "Expires": "90",
      "SipGroup": "sip.ab",
      "Port": "10105",
      "QParam": "1"
    },
    {
      "UserId": "1",
      "Number": "157",
      "Login": "157",
      "Password": "123",
      "SipDomain": "pv.ssw3",
      "Expires": "90",
      "SipGroup": "sip.ab",
      "Port": "10106",
      "QParam": "1"
    }
  ],
  "UserVar": [
    {
     "%%MyDname%%" : "TestCLIP"
    }
  ],
  "PreCoconConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/ss/enable %%0.Number%% clip"
    }
  ],
  "PostCoconConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/ss/deactivate %%0.Number%% clip",
      "command2": "/domain/%%DEV_DOM%%/ss/disable %%0.Number%% clip"
    }
  ],

"Tests": [
    {
      "Name": "chk_anonymous_call",
      "Description": "Делаем вызов с отключенным CLIP. Проверяем, что номер и dname равны Anonymous",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": "0",
              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -set CHK_DNAME Anonymous -set CHK_NUMBER Anonymous",
                  "SippType": "uas",
                  "NeedAuth": "True",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "1",
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%% -set DISPLAY_NAME %%MyDname%%",
                  "SippType": "uac",
                  "NeedAuth": "True",
                  "Timeout": "20s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "chk_call_with_clip",
      "Description": "Активируем CLIP. На стороне Б ожидаем номер абонента А",
      "TestProcedure": [
        {
          "CoconCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% clip"
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": "0",
              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -set CHK_DNAME \"\" -set CHK_NUMBER %%1.Number%%",
                  "SippType": "uas",
                  "NeedAuth": "True",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": "1",
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%% -set DISPLAY_NAME %%MyDname%%",
                  "SippType": "uac",
                  "NeedAuth": "True",
                  "Timeout": "20s"
                }
              ]
            }
          ]
        },
        {
          "CoconCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/ss/deactivate %%0.Number%% clip"
            }
          ]
        }
      ]
    }
  ]
}