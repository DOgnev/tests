{
  "TestName": "dname_tests",
  "AutoTest": true,
  "Users": [
    {
      "UserId" : 0,
      "Number": "156",
      "Login": "156",
      "Password": "123",
      "SipDomain": "%%DEV_DOM%%",
      "Expires" : 90,
      "SipGroup": "sip.ab",
      "Port" : 10105,
      "QParam" : 1
    },
    {
      "UserId" : 1,
      "Number": "157",
      "Login": "157",
      "Password": "123",
      "SipDomain": "%%DEV_DOM%%",
      "Expires" : 90,
      "SipGroup": "sip.ab",
      "Port" : 10106,
      "QParam" : 1
    }
  ],
  "UserVar": [
    {
      "%%ALERTING_NAME%%" : "\"Вызываемый абонент Test\""
    }
  ],
  "PreConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/ss/enable %%0.Number%% cnip",
      "command2": "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% cnip",
      "command3": "/domain/%%DEV_DOM%%/ss/enable %%0.Number%% clip",
      "command4": "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% clip",
      "command5": "/domain/%%DEV_DOM%%/ss/enable %%1.Number%% cnip",
      "command6": "/domain/%%DEV_DOM%%/ss/activate %%1.Number%% cnip",
      "command7": "/domain/%%DEV_DOM%%/ss/enable %%1.Number%% clip",
      "command8": "/domain/%%DEV_DOM%%/ss/activate %%1.Number%% clip"
    }
  ],
  "PostConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/ss/deactivate %%0.Number%% cnip",
      "command2": "/domain/%%DEV_DOM%%/ss/disable %%0.Number%% cnip",
      "command3": "/domain/%%DEV_DOM%%/ss/deactivate %%0.Number%% clip",
      "command4": "/domain/%%DEV_DOM%%/ss/disable %%0.Number%% clip",
      "command5": "/domain/%%DEV_DOM%%/ss/deactivate %%1.Number%% cnip",
      "command6": "/domain/%%DEV_DOM%%/ss/disable %%1.Number%% cnip",
      "command7": "/domain/%%DEV_DOM%%/ss/deactivate %%1.Number%% clip",
      "command8": "/domain/%%DEV_DOM%%/ss/disable %%1.Number%% clip"
    }
  ],

  "Tests": [
    {
      "Name": "send_to_with_dname",
      "Description": "Передаём display_name в поле To: со стороны Б и проверяем его наличие в RPI на стороне А.",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 1,
              "WriteStat" : true,
              "Name": "UAS_WITH_DNAME_IN_TO_HDR",
              "Commands": [
                {
                  "SourceFile": "dname/uas_send_dname_in_to_hdr.xml",
                  "Options": "-m 1 -set DNAME %%ALERTING_NAME%%",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 0,
              "WriteStat" : true,
              "Name": "UAC_CHK_RPI_DNAME",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_bye.xml",
                  "Options": " -m 1 -set CDPN %%1.Number%% -set CDPNDOM %%1.SipDomain%% -set CHK_ALERTING_NAME %%ALERTING_NAME%% -set CHK_ALERTING_NUMBER %%1.Number%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "send_to_wout_dname",
      "Description": "Не передаём display_name в поле To: со стороны Б и проверяем, что в RPI вместо Dname номер.",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 1,
              "WriteStat" : true,
              "Name": "UAS_WOUT_DNAME_IN_TO_HDR",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 2000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 0,
              "WriteStat" : true,
              "Name": "UAC_CHK_RPI_DNAME",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": " -m 1 -set CDPN %%1.Number%% -set CDPNDOM %%1.SipDomain%% -set CHK_ALERTING_NAME %%1.Number%% -set CHK_ALERTING_NUMBER %%1.Number%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
