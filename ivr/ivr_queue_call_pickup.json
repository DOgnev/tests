      "command2": "/domain/%%DEV_DOM%%/cc/agent/remove %%1.Number%%",
      "command3": "/domain/%%DEV_DOM%%/cc/agent/remove %%2.Number%%",
      "command4": "/domain/%%DEV_DOM%%/cc/queue/remove %%QUEUE_NAME%%",
      "command5": "/domain/%%DEV_DOM%%/ss/deactivate %%0.Number%% cc_agent",
      "command6": "/domain/%%DEV_DOM%%/ss/disable %%0.Number%% cc_agent",
      "command7": "/domain/%%DEV_DOM%%/ss/deactivate %%1.Number%% cc_agent",
      "command8": "/domain/%%DEV_DOM%%/ss/disable %%1.Number%% cc_agent",
      "command9": "/domain/%%DEV_DOM%%/ss/deactivate %%2.Number%% cc_agent",
      "command10": "/domain/%%DEV_DOM%%/ss/disable %%2.Number%% cc_agent"
    }
  ],
  "Tests": [
    {
      "Name": "queue_direct_call_fail_pickup",
      "Description": "Проверка того, что агент не может перехватить вызов в случае отключенного pickup",
      "TestProcedure": [
        {
          "ServiceFeature": [
            {
              "code": "*160*%%0.Number%%*%%AGENT_PASS%%#",
              "userId": 0
            },
            {
              "code": "*160*%%1.Number%%*%%AGENT_PASS%%#",
              "userId": 1
            },
            {
              "code": "*160*%%2.Number%%*%%AGENT_PASS%%#",
              "userId": 2
            }
          ]
        },
        {
          "Sleep" : 2
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "WriteStat": true,
              "BackGround" : true,
              "Name": "UAS_RECV_CANCEL",
              "Commands": [
                {
                  "SourceFile": "ivr/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 3,
              "BackGround" : true,
              "WriteStat": true,
              "Name": "UAC_MAKE_DIRECT_CALL",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_cancel.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%% -aa -d 14000",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            }
          ]
        },
        {"Sleep" : 5},
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 2,
              "WriteStat": true,
              "Name": "UAC_FAIL_PICKUP_DIRECT_CALL",
              "Commands": [
                {
                  "SourceFile": "ivr/uac_recv_480.xml",
                  "Options": "-m 1 -set CDPN *08# -set CDPNDOM %%0.SipDomain%% -aa",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            }
          ]
        },
        {"Sleep" : 10}
      ]
    },
    {
      "Name": "queue_direct_call_succ_pickup",
      "Description": "Проверка перехвата прямого вызова между агентами КЦ",
      "TestProcedure": [
        {
          "ServiceFeature": [
            {
              "code": "*160*%%0.Number%%*%%AGENT_PASS%%#",
              "userId": 0
            },
            {
              "code": "*160*%%1.Number%%*%%AGENT_PASS%%#",
              "userId": 1
            },
            {
              "code": "*160*%%2.Number%%*%%AGENT_PASS%%#",
              "userId": 2
            }
          ]
        },
        {
          "Sleep" : 2
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "WriteStat": true,
              "BackGround" : true,
              "Name": "UAS_RECV_CANCEL",
              "Commands": [
                {
                  "SourceFile": "ivr/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 3,
              "BackGround" : true,
              "WriteStat": true,
              "Name": "UAC_MAKE_DIRECT_CALL",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%% -set DISPLAY_NAME %%CLIENT_DNAME%% -aa -set CHK_ALERTING_NAME %%AGENT0_NAME%% -set CHK_CONNECTED_NAME %%AGENT1_NAME%% -set CHK_ALERTING_NUMBER %%0.Number%% -set CHK_CONNECTED_NUMBER %%1.Number%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            }
          ]
        },
        {"Sleep" : 5},
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
              "Name": "UAC_PICKUP_DIRECT_CALL",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_bye.xml",
                  "Options": "-m 1 -set CDPN *08# -set CDPNDOM %%0.SipDomain%% -set CHK_CONNECTED_NUMBER %%3.Number%% -set CHK_CONNECTED_NAME %%CLIENT_DNAME%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            }
          ]
        },
        {"Sleep" : 5}
      ]
