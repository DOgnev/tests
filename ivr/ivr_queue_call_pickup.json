      "%%QUEUE_NAME%%": "ivr_queue_pickup",
      "%%AGENT_PASS%%": "1111"
    }
  ],
  "PreConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/cc/agent/declare %%0.Number%% password = %%AGENT_PASS%%",
      "command2": "/domain/%%DEV_DOM%%/cc/agent/declare %%1.Number%% password = %%AGENT_PASS%%",
      "command3": "/domain/%%DEV_DOM%%/cc/agent/declare %%2.Number%% password = %%AGENT_PASS%%",
      "command4": "/domain/%%DEV_DOM%%/cc/queue/declare %%QUEUE_NAME%% agents = [agent:%%0.Number%%,agent:%%1.Number%%,agent:%%2.Number%%]",
      "command5": "/domain/%%DEV_DOM%%/ss/enable %%0.Number%% cc_agent",
      "command6": "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% cc_agent",
      "command7": "/domain/%%DEV_DOM%%/ss/enable %%1.Number%% cc_agent",
      "command8": "/domain/%%DEV_DOM%%/ss/activate %%1.Number%% cc_agent",
      "command9": "/domain/%%DEV_DOM%%/ss/enable %%2.Number%% cc_agent",
      "command10": "/domain/%%DEV_DOM%%/ss/activate %%2.Number%% cc_agent",
      "command11": "/domain/%%DEV_DOM%%/cc/agent/set %%0.Number%% pickup true ",
      "command12": "/domain/%%DEV_DOM%%/cc/agent/set %%1.Number%% pickup true ",
      "command13": "/domain/%%DEV_DOM%%/cc/agent/set %%0.Number%% display_name %%AGENT0_NAME%%",
      "command14": "/domain/%%DEV_DOM%%/cc/agent/set %%1.Number%% display_name %%AGENT1_NAME%%",
      "command15": "sleep 10"
    }
  ],
  "PostConf": [
    {
