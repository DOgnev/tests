    {
      "Name": "Test_without_clir",
      "Description": "Делаем анонимный вызов без активированного clir. DNAME и номер пробрасываются на сторону Б",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 1,
              "Name": "UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1 -set CHK_DNAME \"\" -set CHK_NUMBER %%0.Number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 0,
              "Name": "ANONYMOUS_UAC",
              "Commands": [
                {
                  "SourceFile": "ss_clir/uac_anonymous.xml",
                  "Options": "-m 1 -set CDPN %%1.Number%% -set CDPNDOM %%1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            }
          ]
        }
